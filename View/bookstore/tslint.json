{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-debugger": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "comment-format": false,
    "curly": false,
    "ordered-imports": false
  }
}
